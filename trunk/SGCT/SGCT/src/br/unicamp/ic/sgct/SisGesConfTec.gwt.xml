<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">
<module rename-to='sisgesconftec'>
	<!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit the default GWT style sheet.  You can change       -->
	<!-- the theme of your GWT application by uncommenting          -->
	<!-- any one of the following lines.                            -->
	<!-- 	<inherits name='com.google.gwt.user.theme.standard.Standard' />  -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
	<inherits name='com.google.gwt.user.theme.chrome.Chrome'/>

	<!-- Inherit the GWTiger library                  -->
	<inherits name='org.gwtiger.gwtiger' />

	<inherits name="com.google.gwt.http.HTTP"/>

	<!-- Specify the app entry point class.                         -->
	<entry-point class='br.unicamp.ic.sgct.client.SisGesConfTec' />

	<servlet path="/inscricao"
		class="br.unicamp.ic.sgct.server.dominio.servicos.InscricaoServiceImpl" />

	<servlet path="/cancelamento"
		class="br.unicamp.ic.sgct.server.dominio.servicos.CancelamentoServiceImpl" />

	<servlet path="/info-geral"
		class="br.unicamp.ic.sgct.server.dominio.servicos.InfoGeralServiceImpl" />

	<servlet path="/conferencia"
		class="br.unicamp.ic.sgct.server.dominio.servicos.ConferenciaServiceImpl" />
</module>